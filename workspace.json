{
  "version": 2,
  "projects": {
    "lab": {
      "root": "apps/lab",
      "sourceRoot": "apps/lab/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/lab",
            "index": "apps/lab/src/index.html",
            "main": "apps/lab/src/main.tsx",
            "polyfills": "apps/lab/src/polyfills.ts",
            "tsConfig": "apps/lab/tsconfig.app.json",
            "assets": ["apps/lab/src/favicon.ico", "apps/lab/src/assets"],
            "styles": ["apps/lab/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/lab/src/environments/environment.ts",
                  "with": "apps/lab/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "lab:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "lab:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/lab/tsconfig.app.json", "apps/lab/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/lab/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/lab/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/lab"]
        }
      }
    },
    "lab-e2e": {
      "root": "apps/lab-e2e",
      "sourceRoot": "apps/lab-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/lab-e2e/cypress.json",
            "tsConfig": "apps/lab-e2e/tsconfig.e2e.json",
            "devServerTarget": "lab:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "lab:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/lab-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/lab-e2e/**/*"]
          }
        }
      }
    },
    "utils-react": {
      "root": "libs/utils/react",
      "sourceRoot": "libs/utils/react/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utils/react",
            "tsConfig": "libs/utils/react/tsconfig.lib.json",
            "project": "libs/utils/react/package.json",
            "entryFile": "libs/utils/react/src/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [{ "glob": "README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utils/react/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils/react"],
          "options": {
            "jestConfig": "libs/utils/react/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "utils-react-akita": {
      "root": "libs/utils/react-akita",
      "sourceRoot": "libs/utils/react-akita/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utils/react-akita",
            "tsConfig": "libs/utils/react-akita/tsconfig.lib.json",
            "project": "libs/utils/react-akita/package.json",
            "entryFile": "libs/utils/react-akita/src/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [{ "glob": "README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utils/react-akita/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils/react-akita"],
          "options": {
            "jestConfig": "libs/utils/react-akita/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "utils-angular": {
      "projectType": "library",
      "root": "libs/utils/angular",
      "sourceRoot": "libs/utils/angular/src",
      "prefix": "mio",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/utils/angular/tsconfig.lib.json",
            "project": "libs/utils/angular/ng-package.json",
            "buildableProjectDepsInPackageJsonType": "dependencies"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/utils/angular/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/utils/angular/tsconfig.lib.json", "libs/utils/angular/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/utils/angular/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utils/angular/jest.config.js",
            "passWithNoTests": true,
            "setupFile": "libs/utils/angular/src/test-setup.ts"
          },
          "outputs": ["coverage/libs/utils/angular"]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-jsx",
        "linter": "eslint",
        "babel": true
      },
      "component": { "style": "styled-jsx" },
      "library": { "style": "styled-jsx", "linter": "eslint" }
    }
  },
  "defaultProject": "lab"
}
